    "cleye": "^1.3.2",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "ini": "^4.1.1",
    "openai": "^4.24.1",
    "picocolors": "^1.0.0",
    "pkgroll": "^2.0.1"
  },
  "devDependencies": {
    "husky": "^4.3.8",

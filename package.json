    "polyglot",
    "lexicon",
    "babel",
    "gloss",
    "vernacular",
    "dialect",
    "jargon",
    "parlance",
    "multilingual",
    "swiftui",
    "अनुवाद",
    "क्रोनोमीट्री",
    "हिंदू",
    "कंकड़ स्विफ्ट",
    "traducción",
    "clima",
    "multilingue",
    "语言学",
    "翻译",
    "转译",
    "针灸治疗",
    "关键词",
    "快速",
    "beruang",
